<p id="notice"><%= notice %></p>

<h1>Phone Transactions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phone_transactions.each do |phone_transaction| %>
      <tr>
        <td><%= link_to 'Show', phone_transaction %></td>
        <td><%= link_to 'Edit', edit_phone_transaction_path(phone_transaction) %></td>
        <td><%= link_to 'Destroy', phone_transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phone Transaction', new_phone_transaction_path %>
