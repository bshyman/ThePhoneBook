<p id="notice"><%= notice %></p>

<h1>Phone Tranzactions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phone_tranzactions.each do |phone_tranzaction| %>
      <tr>
        <td><%= link_to 'Show', phone_tranzaction %></td>
        <td><%= link_to 'Edit', edit_phone_tranzaction_path(phone_tranzaction) %></td>
        <td><%= link_to 'Destroy', phone_tranzaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phone Tranzaction', new_phone_tranzaction_path %>
