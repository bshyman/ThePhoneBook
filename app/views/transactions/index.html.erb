<p id="notice"><%= notice %></p>

<h1>Tranzactions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tranzactions.each do |tranzaction| %>
      <tr>
        <td><%= link_to 'Show', tranzaction %></td>
        <td><%= link_to 'Edit', edit_tranzaction_path(tranzaction) %></td>
        <td><%= link_to 'Destroy', tranzaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tranzaction', new_tranzaction_path %>
